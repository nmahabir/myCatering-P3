{
  "name": "my-catering",
  "version": "0.1.0",
  "private": true,
  "main": "server.js",
  "dependencies": {
    "axios": "^0.16.2",
    "express": "^4.16.3",
    "firebase": "^5.5.9",
    "if-env": "^1.0.4",
    "mongoose": "^5.0.17",
    "react": "^16.6.2",
    "react-dom": "^16.6.2",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
    // "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": "8.11.4"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
